./getnPlot.py --date 2022-12-15 --time 00:04 --kind lahar --pre 10m --dur 60m --fscale log --fmin 1.0 --fmax 20.0 --tag test016g_lahar_log_fed
./getnPlot.py --date 2022-12-15 --time 00:04 --kind lahar --pre 10m --dur 60m --zscale sqrt --fscale log --fmin 1.0 --fmax 100.0 --tag test016h_sqrt_lahar_log_fed
