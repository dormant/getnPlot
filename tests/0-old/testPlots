./getnPlot.py --date 2022-12-15 --time 00:04 --kind lahar --pre 10m --dur 60m --plotnorms --tag test016b_lahar_norms
./getnPlot.py --date 2022-12-15 --time 00:04 --kind lahar --pre 10m --dur 60m --plotnorms --fmin 5.0 --fmax 10.0 --tag test016c_lahar_norms_fed
