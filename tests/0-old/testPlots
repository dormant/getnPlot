./getnPlot.py --date 2023-01-19 --time 11:17:43 --kind Z --fscale log --plotspec --tag test011c_Z_log_plotspec
./getnPlot.py --date 2023-01-19 --time 11:17:43 --kind Z --zscale amp --fscale log --plotspec --tag test011d_Z_log_plotspec
./getnPlot.py --date 2023-01-19 --time 11:17:43 --kind Z --zscale power --fscale log --plotspec --tag test011e_Z_log_plotspec
./getnPlot.py --date 2023-01-19 --time 11:17:43 --kind Z --zscale log --fscale log --plotspec --tag test011f_Z_log_plotspec
./getnPlot.py --date 2023-01-19 --time 11:17:43 --kind Z --zscale sqrt --fscale log --plotspec --tag test011g_Z_log_plotspec
